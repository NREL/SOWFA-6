FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      qwall;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../setUp"

dimensions      [ 0 1 -1 1 0 0 0 ];

internalField   uniform (0.0 0.0 0.0);

boundaryField
{
    lower
    {
        type            specifiedSurfaceTemperature;
        TSurface        table
        (
            ( 0       $TRef)
            ( 100000  #calc "$TRef + 100000*$heatingRate" )
            # #include "../drivingData/surfaceSkinPotentialTemperatureTable"
        );
        kappa           $kappa;
        z0              uniform $z0;
        betaM           $betaM;
        gammaM          $gammaM;
        betaH           $betaH;
        gammaH          $gammaH;
        alphaH          $alphaH;
        averageType     $wallModelAverageType;
        value           uniform (0 0 0);
    }
    upper
    {
        type            fixedValue;
        value           $internalField;
    }
    west 
    {
        type            cyclic;
    }
    east 
    {
        type            cyclic;
    }
    north 
    {
        type            cyclic;
    }
    south
    {
        type            cyclic;
    }
}


