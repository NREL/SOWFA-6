FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      kappat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    lower
    {
        type            cyclic;
    }
    upper
    {
        type            cyclic;
    }
    west
    {
        type            zeroGradient;
    }
    east
    {
        type            zeroGradient;
    }
    north
    {
        type            cyclic;
    }
    south
    {
        type            cyclic;
    }
}


