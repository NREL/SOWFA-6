FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
   location    "0";
    object      k ;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../setUp"

 
dimensions      [0 2 -2 0 0 0 0];
 
internalField   uniform $k0;

boundaryField
{
    lower
    {
        type            zeroGradient;
    }
    upper
    {
        type            zeroGradient;
    }
    east
    {
        type            zeroGradient;
    }
    west
    {
        type            fixedValue;
        value           uniform $k0;
    }
    south
    {
        type            zeroGradient;
    }
    north
    {
        type            zeroGradient;
    }
}


